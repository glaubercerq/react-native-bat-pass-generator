{
  "expo": {
    "name": "Bat Pass Generator",
    "slug": "bat-pass-generator",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/update-url-placeholder"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#000000"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "backgroundColor": "#000000"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "backgroundColor": "#000000"
    },
    "scheme": "bat-pass-generator",
    "platforms": ["ios", "android", "web"]
  }
}
